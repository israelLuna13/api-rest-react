{
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
   //   "lib": ["ESNext"],
      "target": "ES2016",
      "module": "CommonJS",//"ESNext" //Modules
     // "moduleResolution": "NodeNext",
      "strict": false,
      "sourceMap": true,
      "esModuleInterop": true,
      "declaration": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "src/__tests__/**/*.ts", "src/__tests__/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  
  